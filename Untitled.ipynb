   "source": [
    "# How to calculate the leap year\n",
    "year = int(input(\"Enter Your Year :\"))\n",
    "# 2024\n",
    "if(year%400 == 0) or  (year % 100 != 0) and  (year % 4 == 0):\n",
    "    print(\"Leap year\")\n",
    "else:\n",
    "    print(\"Not leap year\")\n"
